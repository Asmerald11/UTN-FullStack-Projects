<table style="border:3px solid #7A7AC2; width: 50%; margin-left: 25%; margin-right: 25%; margin-top: 20px;">
    <tbody style="border:1px solid #7A7AC2;">
            <tr>
                <th style="border:3px solid #7A7AC2; text-align:center;">Usuario</th>
                <th style="border:3px solid #7A7AC2; text-align:center;">Mensaje</th>
            </tr>
        {{#each data}}
            <tr>
                <td style="border:3px solid #7A7AC2;">{{this.usuario}}</td>
                <td style="border:3px solid #7A7AC2;">{{this.comentario}}</td>
            </tr>
        {{/each}}
